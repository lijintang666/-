/* header */
.search{
      width:100%;
      height: 160rpx;
      background: #008DFF;
      padding:0 20rpx; 
      color:#fff;
     
}
.search .search_title{
      font-size: 36rpx;  
      height: 80rpx;     
      line-height: 80rpx;
}

.search .search_con{
      width: 100%;
      height: 60rpx;
      background: #fff;
      text-align: center;
      font-size: 28rpx;
      line-height: 60rpx;
      color:#999;
}
.jt{
      display: inline-block;
      border-top:10rpx solid #fff;
      border-right:10rpx solid transparent;
      border-left:10rpx solid transparent;
      border-bottom:10rpx solid transparent;
}
/* header end */

/* category */
.category{
width: 100%;
height: 400rpx;
}
.category .category_item{
float: left;
width: 20%;
height: 200rpx;
text-align: center;
}
.category .category_item image{
margin-top:16rpx;
width: 60%;
}
.category .category_item .category_name{
font-size: 28rpx;
color:#666;
}
.mySwiper{
      height: 400rpx;
}
/* category end*/
.quality{
      margin: 0 20rpx;
      height: 200rpx;
      background: linear-gradient(0deg,#f4f4f4 5%,#fafafa 95%);
}
.quality .con{
      padding: 30rpx 0 0 30rpx;
      position: relative;
}
.quality .conF{
      font-weight: 700;
      font-size: 36rpx;
}
.quality .conS{
      font-size: 28rpx;
      color:#666;
      
}
.quality .conT{
      font-size: 28rpx;
      color:#af8260;
      margin-top:10rpx; 
}
.quality .con_bg{
      position: absolute;
      top: 0;
      right: 0;
      width: 300rpx;
}
.quality .con_bg image{
      width: 100%;
      
}
/* category end */
/* members会员 */

.members{
      margin: 10rpx 20rpx;
      height: 80rpx;
      background-image: linear-gradient(90deg,#ffefc4,#f3dda0);
      line-height: 80rpx;
      font-size: 28rpx;
      padding-left:80rpx; 
      padding-right: 20rpx;
      position: relative;
}
.members .member_icon{
      width: 40rpx;
      height: 40rpx;
      position: absolute;
      top:10rpx;
      left:20rpx;
}
.members .member_icon image{
      width: 100%;
}
.members .members_con{
      color:#644f1b;
}
.members .member_textL{
      float: left;
}
.members .member_textL .chaoji{
      color: #644f1b;
      font-size: 32rpx;
      font-weight: 700;
}
.members .member_textL .dian {
      display: inline-block;
      width: 4rpx;
      height: 4rpx;
      background: #333;
      margin: 0 10rpx;
      vertical-align: middle;
}
.members .member_textR{
      float: right;
} 

/* members end*/
/* banner */
.banner_swiper{
      margin: 0 20rpx;
      height: 190rpx;
}
.banner_swiper .banner_item{
      width: 100%;
      height: 190rpx;

}
.banner_swiper .banner_item image{
      width: 100%;
}
/* banner end */
/* goods_title */
.goods_title{
      height: 80rpx;
      text-align: center;
      line-height: 80rpx;
      font-size: 32rpx;
      margin: 0 20rpx;
}
.goods_title .title{
      display: inline-block;
      margin: 0 30rpx;
}
.goods_title .line{
      display: inline-block;
      width: 60rpx;
      height: 2rpx;
      background: #ccc;
      vertical-align: middle;
}
/* goods_title end */
/* category */
.category_nav{
      width: 100%;
      height: 80rpx;
      line-height: 80rpx;
      text-align: center;
      color:#666;
      font-size:32rpx;
}
.category_nav text{
      padding: 0 36rpx;
}
.category_nav .active{
      font-size:32rpx;
      color: #333;
      font-weight: 700;
}
/* category end*/
/* shangjia */
.shangjia{
padding:20px 10px;
display: flex;
}
.shangjia .shangjia_icon{
width: 140rpx;
}
.shangjia .shangjia_icon image{
width: 100%;
}
.shangjia_con {
      flex: 1;
      padding: 0 10rpx;
       font-size: 24rpx;
      color: #666;
}
.shangjia_con .sj_title{
      font-weight: 700;
      font-size: 34rpx;
      color: #333;
}
.shangjia_con .sj_con{
     
}
.shangjia_con .sj_info{
      width: 100%;
      text-align: right;
}
.shangjia_con .sj_info text{
      padding: 0 12rpx;
}
.shangjia_con .sj_style{
      border: 1px solid #999;
      float: left;
}
/* shangjia end*/
/* load加载 */
.load{
      width: 100%;
      text-align: center;
      color: #666;
      font-size: 28rpx;
      height: 60rpx;
      line-height: 60rpx;
}

.load  image{
      width: 50rpx;
      vertical-align: middle;
      animation: scoll linear infinite 1s;
}
/* 定义动画 */
@keyframes scoll {
      0%{
            transform: rotate(0deg)
      }
      100%{
            transform: rotate(360deg)
      }
}
/* load加载 end*/